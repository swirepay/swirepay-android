<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExpandableLayout">
    <attr format="boolean" name="expandable_isExpanded"/>
    <attr format="reference" name="expandable_parentLayout"/>
    <attr format="reference" name="expandable_secondLayout"/>
    <attr format="integer" name="expandable_duration"/>
    <attr format="integer" name="expandable_spinner"/>
    <attr format="boolean" name="expandable_showSpinner"/>
    <attr format="boolean" name="expandable_spinner_animate"/>
    <attr format="integer" name="expandable_spinner_rotation"/>
    <attr format="dimension" name="expandable_spinner_size"/>
    <attr format="dimension" name="expandable_spinner_margin"/>
    <attr format="color" name="expandable_spinner_color"/>
    <attr format="enum" name="expandable_spinner_gravity">
      <enum name="start" value="0"/>
      <enum name="end" value="1"/>
    </attr>
    <attr format="enum" name="expandable_animation">
      <enum name="normal" value="0"/>
      <enum name="accelerate" value="1"/>
      <enum name="bounce" value="2"/>
      <enum name="overshoot" value="3"/>
    </attr>
  </declare-styleable>
</resources>